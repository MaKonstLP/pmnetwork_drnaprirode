<?xml version="1.0" encoding="UTF-8"?>
    <urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"> 
        <url>
            <loc>{{host}}</loc>
            <priority>1</priority>
        </url>
        <url>
            <loc>{{host}}/katalog-ploshchadok/</loc>
            <priority>0.9</priority>
        </url>

        <url>
            <loc>{{host}}/kontakty/</loc>
            <priority>0.9</priority>
        </url>

        {% for slice in slices %}
        	<url>
	            <loc>{{host}}/{{slice.alias}}/</loc>
                <priority>0.8</priority>
	        </url>
        {% endfor %}

        <url>
            <loc>{{host}}/privacy-policy/</loc>
            <priority>0.9</priority>
        </url>

        {% for item in items %}
        	<url>
	            <loc>{{host}}/katalog-ploshchadok/{{item.restaurant_slug}}/</loc>
                <priority>0.8</priority>
	        </url>
        {% endfor %}
        
    </urlset>